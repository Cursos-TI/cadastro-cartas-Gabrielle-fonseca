    printf("digite o nome da cidade: \n");
    scanf("%s, &cidade");

    printf(" digite a população");
    scanf("%d, &população");

    printf("digite a aea da cidade; \n");
    scanf("%f, &area");

    printf(" digite o numero de pontos tuisticos");
    scanf("%d, &pontos_tuisticos");



return 0;
}
